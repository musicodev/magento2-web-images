<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<musicodev_webimages>
			<extensions>
				<vector>
					<svg>svg</svg>
					<svg_xml>svg+xml</svg_xml>
				</vector>
				<web_image>
					<avif>avif</avif>
					<webp>webp</webp>
				</web_image>
			</extensions>
		</musicodev_webimages>
		<dev>
			<image>
				<adapters>
					<GD2>
						<class>MusicoDev\WebImages\Model\Image\Adapter\Gd2</class>
					</GD2>
				</adapters>
			</image>
		</dev>
	</default>
</config>
